			ErrorID						=> stMcAxisOnline.mcSS2.udiErrorID);
			
mcStopSS1(	Axis						:= Axis,
			Execute						:= stMcAxisOnline.mcSS1.bExecute,
			Deceleration				:= stMcAxisOnline.mcSS1.lrDeceleration,
			Jerk						:= stMcAxisOnline.mcSS1.lrJerk,
